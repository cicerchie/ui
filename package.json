{
  "name": "@cicerchie/ui",
  "version": "0.5.3",
  "description": "A set of web components for building fast user interfaces.",
  "author": "https://github.com/cicerchia",
  "license": "MIT",
  "homepage": "https://github.com/cicerchie/ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cicerchie/ui.git"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/cicerchia"
  },
  "bugs": {
    "url": "https://github.com/cicerchie/ui/issues"
  },
  "keywords": [
    "svelte ui",
    "svelte",
    "tailwind ui",
    "tailwindcss",
    "web components",
    "component library",
    "design system"
  ],
  "main": "index.js",
  "svelte": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "rm -rf build/* && svelte-kit build",
    "package": "rm -rf package/* && svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "storybook": "cd storybook & npm run dev",
    "postversion": "cp -r package.json .. && cp -r CHANGELOG.md .. && cd .. && npm run package && cd ./package"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@sveltejs/adapter-static": "1.0.0-next.16",
    "@sveltejs/kit": "1.0.0-next.153",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "babel-plugin-optimize-objstr": "1.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.0",
    "obj-str": "1.1.0",
    "prettier": "2.3.2",
    "prettier-plugin-svelte": "2.3.1",
    "svelte": "3.42.1",
    "svelte-check": "2.2.4",
    "svelte-preprocess": "4.7.4",
    "svelte2tsx": "0.4.5",
    "tslib": "2.3.1",
    "typescript": "4.3.5"
  },
  "type": "module"
}
