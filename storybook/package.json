{
    "name": "@cicerchie/ui-storybook",
    "version": "0.0.0",
    "scripts": {
        "build": "npm run generate:tailwind && build-storybook",
        "dev": "npm run generate:tailwind && start-storybook -p 6006",
        "generate:tailwind": "npx tailwindcss-cli build -c ../tailwind.config.cjs -o ./src/style/tailwind.css"
    },
    "devDependencies": {
        "@storybook/addon-actions": "6.3.12",
        "@storybook/addon-essentials": "6.3.12",
        "@storybook/addon-links": "6.3.12",
        "@storybook/addon-svelte-csf": "1.1.0",
        "@storybook/builder-webpack5": "6.3.12",
        "@storybook/manager-webpack5": "6.3.12",
        "@storybook/svelte": "6.3.12",
        "postcss": "8.3.11",
        "svelte": "3.44.1",
        "svelte-loader": "3.1.2",
        "svelte-preprocess": "4.9.8",
        "tailwindcss": "2.2.19",
        "tailwindcss-cli": "0.1.2",
        "typescript": "4.4.4"
    }
}
