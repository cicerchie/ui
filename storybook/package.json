        "@storybook/addon-svelte-csf": "2.0.7",
        "@storybook/builder-webpack5": "6.5.10",
        "@storybook/manager-webpack5": "6.5.10",
        "@storybook/svelte": "6.5.10",
        "postcss": "8.4.16",
        "svelte": "3.50.0",
        "svelte-loader": "3.1.3",
        "svelte-preprocess": "4.10.7",
        "tailwindcss": "3.1.8",
        "typescript": "4.8.2"
