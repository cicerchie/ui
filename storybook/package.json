        "@storybook/builder-webpack5": "6.5.10",
        "@storybook/manager-webpack5": "6.5.16",
        "@storybook/svelte": "6.5.10",
        "postcss": "8.4.21",
        "svelte": "3.58.0",
        "svelte-loader": "3.1.7",
        "svelte-preprocess": "5.0.3",
        "tailwindcss": "3.3.1",
        "typescript": "5.0.4"
