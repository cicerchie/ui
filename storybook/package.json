{
    "name": "@cicerchie/ui-storybook",
    "version": "0.0.0",
    "scripts": {
        "build": "npm run generate:tailwind && build-storybook",
        "dev": "npm run generate:tailwind && start-storybook -p 6006",
        "generate:tailwind": "npx tailwindcss-cli build -c ../tailwind.config.js -o ./src/style/tailwind.css"
    },
    "devDependencies": {
        "@storybook/addon-actions": "6.2.1",
        "@storybook/addon-essentials": "6.2.1",
        "@storybook/addon-links": "6.2.1",
        "@storybook/addon-svelte-csf": "1.0.0",
        "@storybook/svelte": "6.2.1",
        "postcss": "8.2.9",
        "svelte-loader": "3.0.0",
        "svelte-preprocess": "4.7.0",
        "tailwindcss-cli": "0.1.2"
    }
}
