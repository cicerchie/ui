        "@storybook/builder-webpack5": "6.4.19",
        "@storybook/manager-webpack5": "6.4.19",
        "@storybook/svelte": "6.4.19",
        "postcss": "8.4.6",
        "svelte": "3.46.4",
        "svelte-loader": "3.1.2",
        "svelte-preprocess": "4.10.4",
